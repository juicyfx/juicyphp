{
  "name": "juicyfx/juicy",
  "description": "\uD83D\uDC18 PHP client for serverless juicy functions",
  "keywords": ["juicyfx", "juicy", "php", "serverless", "lambda", "zeit", "now", "zeit-now"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/juicyfx/juicy",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "guzzlehttp/guzzle": "^6.5.5 || ^7.2.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.3",
    "ninjify/nunjuck": "^0.4.0",
    "ninjify/qa": "^0.12.0 || ^0.13.0",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "JuicyFx\\Juicy\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\": "tests/cases"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  }
}
